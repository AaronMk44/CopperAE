      <HBox prefHeight="72.0" prefWidth="605.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Settings">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets left="50.0" top="10.0" />
         </padding>
      </HBox>
   </top>
   <center>
      <ScrollPane fitToHeight="true" fitToWidth="true" pannable="true" prefHeight="800.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <content>
            <VBox fx:id="container" prefWidth="700.0" />
         </content>
      </ScrollPane>
   </center>
</BorderPane>
